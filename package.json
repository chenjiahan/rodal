{
  "name": "rodal",
  "version": "2.0.1",
  "description": "A React modal with animations.",
  "main": "lib/rodal.js",
  "module": "lib/rodal.esm.js",
  "dependencies": {
    "prop-types": "^15.6.0",
    "classnames": "^2.2.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2"
  },
  "peerDependencies": {
    "react": ">= ^17.0.0 || >= ^18.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "dev": "node_modules/.bin/webpack-dev-server",
    "build": "webpack -p --hide-modules",
